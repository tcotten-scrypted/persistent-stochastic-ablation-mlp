# AWS SageMaker Configuration
# Copy this file to .env and fill in your actual values

# Required AWS credentials and configuration
AWS_SAGEMAKER_ROLE=arn:aws:iam::YOUR_ACCOUNT_ID:role/YOUR_SAGEMAKER_ROLE
AWS_ACCOUNT_ID=YOUR_ACCOUNT_ID
AWS_REGION=us-east-1

# S3 Configuration
S3_BUCKET_NAME=your-training-bucket-name
S3_PREFIX=psa-experiment

# SageMaker Instance Configuration
INSTANCE_TYPE=ml.g4dn.xlarge
USE_SPOT_INSTANCES=false
MAX_RUN_SECONDS=172800
